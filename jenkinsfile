node{
   stage('SCM checkout'){
    
    git 'https://github.com/kaifakh22/my-app-jenkinsfile.git'
   }
   stage('compile-package')
   {
    def mvnHome = tool name: 'Maven 3.8.5', type: 'maven'
    sh "${mvnHome}/bin/mvn package"
   }
   stage('Email Notification'){
     mail bcc: '', body: '''Hi,This email is from jenkins pipeline job''', cc: '', from: '', replyTo: '', subject: 'Jenkins pipeline job', to: 'awscloud.akhtar@gmail.com'
   }
    

}
